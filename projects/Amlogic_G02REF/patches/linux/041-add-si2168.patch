+					printk("[%s] the func of set_frontend err.\n",__func__);
+					return  DVBFE_ALGO_SEARCH_FAILED;
+				}
+			}
+		}	               
+        }
+        
+	   return DVBFE_ALGO_SEARCH_FAILED;               
+}
