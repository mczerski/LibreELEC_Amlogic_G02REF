+	nmi_tuner_os_log("param->ucBw:%d\n",param->ucBw);	
+	nmi_tuner_os_log("param->tvstd:%d\n",param->tvstd);	
+
+	switch(param->tvstd)
+	{
+		case NMI_NTSC: 
+			{
+			    //add codes	
+				tune.std = nNTSC;
+			}
+			break;
+		case NMI_PAL_G: 
+			{
+			        tune.std = nPAL_G;	
+			}
+			break;
+		case NMI_PAL_M: 
+			{
+			    tune.std = nPAL_M;		
+			}
+			break;
+		case NMI_PAL_N: 
+			{
+			    tune.std = nPAL_N;	
+			}
+			break;
+		case NMI_PAL_K: 
+			{
+			    tune.std = nPAL_K;	
+			}
+			break;
+		case NMI_PAL_L: 
+			{
+			    tune.std = nPAL_L;	
+			}
+			break;
+		case NMI_PAL_D: 
+			{
+			     tune.std = nPAL_L;//-----------
+			}
+			break;
+		case NMI_SECAM_L: 
+			{
+			    tune.std = nSECAM_L;	
+			}
+			break;
+		case NMI_SECAM_B: 
+			{
+			   tune.std = nSECAM_B;		
+			}
+			break;
+		case NMI_SECAM_D: 
+			{
+			    tune.std = nSECAM_D;		
+			}
+			break;
+		case NMI_ATSC_8VSB: 
+		case NMI_ATSC_64QAM: 
+		case NMI_ATSC_256QAM: 
+			{
+			    //add codes
+				nmi_tuner_os_log("mode NMI_ATSC %d\n",param->ucBw);
+			    	switch(param->ucBw)
+				{
+					case 0:
+					case 6: tune.std = nDTV_6; break;
+					case 1:
+					case 7: tune.std = nDTV_7; break;
+					case 2:
+					case 8:
+					default:
+					tune.std = nDTV_8; break;
+				}
+			}
+			break;
+		case NMI_DVBT: 
