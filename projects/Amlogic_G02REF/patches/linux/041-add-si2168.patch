+void nm120_rfli_track(tNmiSwrfliArg *p)
+{
+	int action;
+	static uint8_t bblicode = 0;
+	static u17_15_t dagcgain = 0;
+
+	static uint8_t lnagain = 0x3;
+	
+	tTnrGain pg;
+	int16_t tmprssi;
+	uint16_t rssi;
+
+	tNmiPriv		*pPriv = (tNmiPriv *)&chip.priv;
+	//add for LTE test
+	if((pPriv->frequency >778000000-1500000) && (pPriv->frequency < 794000000 + 1500000) && 
+		(pPriv->nmistd == NMI_DVBT || pPriv->nmistd == NMI_DVBT2))
+	{		
+		uint8_t v2b = 0x91;
+		uint8_t v0a = 0xf8;		
+		
+		nm131_get_lna_gain(&pg);
+		tmprssi = pg.rssix100/100;
+		if(tmprssi<-100)
+			rssi = 100;
+		else if(tmprssi>0)
+			rssi = 0;
+		else
