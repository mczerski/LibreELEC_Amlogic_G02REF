+					p->frequency -=  ATV_AFC_1_0MHZ;
+					pr_aml("[%s] p->frequency=[%d] over maxafcfreq=[%d].search failed.\n",__func__,p->frequency,maxafcfreq);
+					return DVBFE_ALGO_SEARCH_FAILED;
