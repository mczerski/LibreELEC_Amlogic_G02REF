+void nmi_demod_aci_config(uint8_t aci)
+{
+	uint32_t val32;
+	
+	val32 = rReg32(0x104);
+	
+	if(((val32<<17)>>28) == aci)
+		return;
+		
+	val32 &= ~(0xful << 11);
+	val32 |= (aci << 11);
+
+	wReg32(0x104, val32);
+}
