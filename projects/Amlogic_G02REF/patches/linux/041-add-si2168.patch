+			nmi_tuner_os_log("[NMI] Failed to initialize chip...\n");
+			result = NMI_E_CHIP_INIT;
+			goto _fail_;
+		}
+
+		/**
+		    Get chip id
+		**/
+		pd->chipid = pd->tnr.getchipid();
+		nmi_tuner_os_log("chip id:");
+		print_32hex(pd->chipid);
+		already_init = 1;
+	}
+
+_fail_:
+
+	return result;
