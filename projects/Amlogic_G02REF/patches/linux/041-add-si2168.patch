+	{
+		int i, len;
+		uint8_t *buf;	
+	
+		buf = chip.inp.busTest.buf; 
+		len = chip.inp.busTest.len;
+	
+	
+		if(len > 0x3c) {
+			len = 0x3c;
+		}
+	
+	
+		for(i=0; i<len; i++) {
+			buf[i] = i; 		
+		}
