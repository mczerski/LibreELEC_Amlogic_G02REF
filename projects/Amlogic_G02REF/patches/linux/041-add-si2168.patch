+       default:
+            return "UNKONW TYPE";
+            break;
+    }
+}
+static int __init aml_fe_init(void)
+{
+	return platform_driver_register(&aml_fe_driver);
+}
+
+
+static void __exit aml_fe_exit(void)
+{
+	platform_driver_unregister(&aml_fe_driver);
+}
