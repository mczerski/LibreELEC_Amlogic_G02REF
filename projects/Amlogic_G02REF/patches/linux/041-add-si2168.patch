+static struct aml_dmx *get_dmx_from_src(enum aml_ts_source_t src)
+{
+	int i;
+	struct aml_dvb *dvb = aml_get_dvb_device();
+	struct aml_dmx *dmx = NULL;
+
+	if (dvb) {
+		switch (src) {
+		case AM_TS_SRC_TS0:
+		case AM_TS_SRC_TS1:
+		case AM_TS_SRC_TS2:
+		case AM_TS_SRC_S_TS0:
+		case AM_TS_SRC_S_TS1:
+		case AM_TS_SRC_S_TS2:
+			for (i = 0; i < DMX_DEV_COUNT; i++) {
+				if (dvb->dmx[i].source == src) {
+					dmx = &dvb->dmx[i];
+					break;
+				}
+				break;
+		case AM_TS_SRC_DMX0:
+				/*if (0 > DMX_DEV_COUNT) */
+				/*dmx = &dvb->dmx[0]; */
+				break;
+		case AM_TS_SRC_DMX1:
+				/*if (1 > DMX_DEV_COUNT) */
+				/*dmx = &dvb->dmx[1]; */
+				break;
+		case AM_TS_SRC_DMX2:
+				/*if (2 > DMX_DEV_COUNT) */
+				/*dmx = &dvb->dmx[2]; */
+				break;
+		default:
+				break;
+			}
+		}
+	}
+	return dmx;
+
+}
