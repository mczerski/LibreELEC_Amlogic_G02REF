+int aml_register_fe_drv(aml_fe_dev_type_t type, struct aml_fe_drv *drv)
+{
+	if(drv){
+		struct aml_fe_drv **list = aml_get_fe_drv_list(type);
+		unsigned long flags;
+
+		spin_lock_irqsave(&lock, flags);
+
+		drv->next = *list;
+		*list = drv;
+
+		drv->ref = 0;
+
+		spin_unlock_irqrestore(&lock, flags);
+	}
+
+	return 0;
+}
