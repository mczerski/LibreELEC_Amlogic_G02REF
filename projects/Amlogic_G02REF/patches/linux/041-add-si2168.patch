+
+int NMI120_GetRSSI(uint8_t outputChoice)
+{
+	int rssi;
+
+	rssi = (int)Nmi_Tuner_Interface_GetRSSI();
+	if (1 == outputChoice) // for percent value
+	{
+		if ((rssi<=0) && (rssi>=-100))
+		{
+			rssi = ABS(rssi);
+            if(rssi>85)
+                rssi = 0;
+            else if(rssi > 45)
+                rssi = (85-rssi)*100/40;
+            else
+                rssi = 100;
+		}
+		else
+		{
+			if (rssi > 0)
+			{
+				rssi = 100;
+			}
+
+			if (rssi < -100)
+			{
+				rssi = 0;
+			}
+		}
+	}
+    nmi_tuner_os_log("[NMI]SSI=%d\n",rssi);
+	return rssi;
+}
