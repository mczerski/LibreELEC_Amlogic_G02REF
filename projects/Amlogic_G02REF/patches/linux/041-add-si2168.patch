+#define  ISDVBC(std) (((std)==nDTV_6) || ((std)==nDTV_7) || ((std)==nDTV_8))
+#define ISATV(std)  ( (!ISDTV(std)) && ((std) != nInvalidNmiTvStd) )
+#define ISCVBS(output) ( ((output) == nCvbsSif) || ((output) == nCvbsBBAud) )
