+	static int nm131_tune(tTnrTune *tune)
+	{
+		tNmiPriv		*pp = (tNmiPriv *)&chip.priv;
+	    //nmi_tuner_os_log("nm131_tune:");
+	      tTnrScan scan;
+		print_32hex(chip.priv.chipid);
+
+		if(ISNM131(chip.priv.chipid)) {
+			if(ISCVBS(tune->output)) {
+				if(tune->std == nFM_RADIO) {
+					nm131_tune_internal(tune, nTuneOnly);
+				} else if(!tune->single_tune) {
+					
+	
+					nmi_memcpy(&scan.tune, tune, sizeof(tTnrTune));
+					scan.alocklevel = 0;
+					scan.found = 0;
+					nm131_scan(&scan);		
+	
+					if(scan.found) {			
+						int fqoff;
+	
+						fqoff = (int)nm131_demod_get_freq_offset();
+	
+						if( nmi_abs(fqoff) > 30000) {
+							tune->rf += fqoff;
+	
+							nm131_tune_internal(tune, nTuneOnly);
+						} else {
+						}
+						nm131_notch_spur(); 
+					} else {
+					}
+				} else {
+					nm131_tune_internal(tune, nTuneOnly);
+					nm131_notch_spur(); 
+				}
+			} else {
+				/**
+				if we switch from CVBS to IF, then make sure notch is off. 
+				**/
+				if(ISCVBS(pp->output)) {
+					nm131_notch_ctl(0, 0);
+				}
+				nm131_tune_internal(tune, nTuneOnly);
+			}
+		} else {
+			nm131_tune_internal(tune, nTuneOnly);
+		}
+
+	if(ISCVBS(tune->output) && !tune->single_tune)
+	{
+		return scan.found;
+	}
+		return 0;
+	
+	}
