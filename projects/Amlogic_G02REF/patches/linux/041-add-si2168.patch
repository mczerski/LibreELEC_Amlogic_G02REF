+static uint8_t Nmi_Tuner_Interface_LoopThrough(void)
+{
+	//int result;
+	tTnrInit cfg;
+	tTnrLtCtrl ltCtl;
+	//tTnrStatus status;
+
+	nmi_tuner_os_memset((void *)&cfg, 0, sizeof(tTnrInit));
+
+	ltCtl.enable = TRUE;
+	if(nmi_drv_ctl(NMI_DRV_LT_CTRL, (void *)&ltCtl)<0)
+	{
+		return FALSE;
+	}
+
+	nmi_drv_ctl(NMI_DRV_WAKE_UP_LT, NULL);
+	return TRUE;
+}
