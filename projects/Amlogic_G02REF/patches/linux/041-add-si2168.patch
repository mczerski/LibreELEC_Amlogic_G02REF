+/******************************************************************************
+**
+**	Bus Read/Write Functions
+**
+*******************************************************************************/
+
+uint8_t rReg8(uint8_t adr)
+{
+	uint8_t b[2];
+
+	b[0] = 0;
+	b[1] = (uint8_t)(adr);
+
+	//nmi_bus_write(b, 2); 
+	//nmi_bus_read(b, 1);
+	nmi_tuner_os_bus_write(chip.inp.ai2c,b, 2);
+	nmi_tuner_os_bus_read(chip.inp.ai2c,b, 1);
+	return b[0]; 
+
+}
+
+uint32_t rReg32(uint16_t adr)
+{
+	uint32_t dat;
+	uint8_t b[4];
+
+	b[0] = (uint8_t)(adr >> 8);
+	b[1] = (uint8_t)(adr);
+
+	//nmi_bus_write(chip.inp.ai2c,b, 2);
+	//nmi_bus_read(chip.inp.ai2c,b, 4);
+	nmi_tuner_os_bus_write(chip.inp.ai2c,b, 2);
+	nmi_tuner_os_bus_read(chip.inp.ai2c,b, 4);
+
+	dat =              b[3];
+	dat = (dat << 8) | b[2]; 
+	dat = (dat << 8) | b[1];
+	dat = (dat << 8) | b[0];
+	
+	return dat;
+
+}
+
+void wReg8(uint8_t adr, uint8_t val)
+{
+	uint8_t b[3];
+
+	b[0] = 0;
+	b[1] = (uint8_t)(adr);
+	b[2] = (uint8_t)(val);
+
+	//nmi_bus_write(b, 3);
+	nmi_tuner_os_bus_write(chip.inp.ai2c,b, 3);
+
+
+}
+
+void wReg32(uint16_t adr, uint32_t val)
+{
+	uint8_t b[6];
+
+	b[0] = (uint8_t)(adr >> 8);
+	b[1] = (uint8_t)(adr);
+	b[2] = (uint8_t)(val);
+	b[3] = (uint8_t)(val >> 8);
+	b[4] = (uint8_t)(val >> 16);
+	b[5] = (uint8_t)(val >> 24);
+
+	//nmi_bus_write(b, 6);
+	nmi_tuner_os_bus_write(chip.inp.ai2c,b, 6);
+}
+#ifndef  NMI_TUNER_CALL_DIRECT	
+
+static void wRegBurst(uint32_t adr, uint8_t *bd, int sz)
+{
+#define OVERHEAD (2)
+
+	int retVal;	
+	int i;
+	uint32_t sz2, sz3, blksz = 4;
+	uint8_t b[256+OVERHEAD];
+
+	if (sz > 0x400/* Maximum allowed length for NM131. */) {
+		return; 
+	}
+
+	if( (sizeof(b)/sizeof(b[0])) <= OVERHEAD ) {
+		return;
+	}
+	
+	/**	
+	Warning: make sure LDO bypass setting is correct
+	so that the chip is not damaged.
+	**/
+	if((0x36 >= adr) && (0x36 < (adr+sz))) {
+		
+		if(chip.inp.ldobypass) {
+			bd[0x36-adr] |= (1 << 7);
+		} else {
+			bd[0x36-adr] &= ~(1 << 7);
+		}
+	}
+
+	/* Discover bus block size. */
+	if(chip.inp.hlp.c.getwriteblksz) {
+		blksz  = chip.inp.hlp.c.getwriteblksz() - OVERHEAD;
+		/* Usable block size should be <= size of b buffer for I2C. */
+		if( blksz > ((sizeof(b)/sizeof(b[0]))-OVERHEAD) ) {
+			blksz = (sizeof(b)/sizeof(b[0]))-OVERHEAD;
+		}
+	} else {	
+		/** 
+		If bus block size is unknown, it is safe to that wRegBurst behaves 
+		like wReg8. 
+		**/
+		blksz = 1;
+	}
+
+	retVal = 0;		
+	sz2 = sz;
+	i = 0;
+	while (((int)sz2)  > 0)
+	{
+		if (sz2 <= blksz)
+			sz3 = sz2;
+		else
+			sz3 = blksz;
+		b[0] = (uint8_t)(adr >> 8);
+		b[1] = (uint8_t)(adr);
+		nmi_memcpy(&b[OVERHEAD], &bd[i], sz3*sizeof(b[0]));	
+
+		if (chip.inp.hlp.c.write(chip.inp.ai2c, b, sz3+OVERHEAD) < 0) {
+			chip.buserror = 1;
+			retVal = -1;
+			break;
+		}
+		
+		retVal += sz3;
+		adr +=sz3;
+		sz2 -=sz3;
+		i+= sz3;
+	}
+
+	return;
+}
+#endif
