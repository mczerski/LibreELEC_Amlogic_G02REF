+    front_end->demod     = &(front_end->demodObj    );
+    front_end->first_init_done     = 0;
+ //   front_end->tuner_ter = &(front_end->tuner_terObj);
+    front_end->TER_init_done       = 0;
+    front_end->TER_tuner_init_done = 0;
+    front_end->auto_detect_TER     = 0;
