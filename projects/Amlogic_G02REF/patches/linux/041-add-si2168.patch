+	
+		if (en) {
+			int32_t spur_off;
+			
+			spur_off = frequency - pp->lofrequency; 	
+			nm131_set_notch_frequency(spur_off);
+			nm131_notch_ctl(1, width);
+		} else {
+			nm131_notch_ctl(0, 0);
+		}
+	}
+#else
