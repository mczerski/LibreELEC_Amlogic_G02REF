+        case SILABS_DVB_T : { sprintf(formatted_status, "%s standard       DVB-T\n" , formatted_status); break; }
+        case SILABS_DVB_T2: { sprintf(formatted_status, "%s standard       DVB-T2\n", formatted_status); break; }
